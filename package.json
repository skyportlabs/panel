{
  "name": "skyport",
  "version": "0.3.0",
  "description": "Skyport is an open source panel for managing and operating game servers and applications built using Node.js with Express and Dockerode.",
  "main": "index.js",
  "bin": {
    "skyport": "./exec/bin/index.js"
  },
  "scripts": {
    "start": "node index.js",
    "start:dev": "nodemon index.js",
    "dev": "concurrently \"npm run start\" \"npm run watch\"",
    "watch": "browser-sync start --config bs-config.js",
    "seed": "node exec/seed.js",
    "createUser": "node exec/createUser.js",
    "build:css": "npx tailwindcss -i ./public/tw.conf -o ./public/assets/tailwind.css --watch"
  },
  "author": "skyportlabs",
  "license": "ISC",
  "dependencies": {
    "@keyv/sqlite": "^3.6.7",
    "@keyvhq/core": "^2.1.1",
    "@keyvhq/mysql": "^2.1.6",
    "@tailwindcss/forms": "^0.5.9",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^11.1.2",
    "better-sqlite3-session-store": "^0.1.0",
    "body-parser": "^1.20.3",
    "cat-loggr": "^1.2.2",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "cookie-parser": "^1.4.7",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-basic-auth": "^1.2.1",
    "express-rate-limit": "^7.4.1",
    "express-session": "^1.18.1",
    "express-ws": "^5.0.2",
    "form-data": "^4.0.1",
    "fs": "^0.0.1-security",
    "keyv": "^4.5.4",
    "multer": "^1.4.5-lts.1",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.15",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "qrcode": "^1.5.4",
    "readline": "^1.3.0",
    "speakeasy": "^2.0.0",
    "tailwindcss": "^3.4.14",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "browser-sync": "^2.26.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.7"
  }
}
